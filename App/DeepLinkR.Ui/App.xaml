             xmlns:local="clr-namespace:DeepLinkR.Ui">
    <Application.Resources>
         <ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary>
					<local:Bootstrapper x:Key="Bootstrapper"></local:Bootstrapper>
